menu items
{{>guest-header}}
<div class="container mt-5 ">
    <h2>All Items</h2>
   {{#each menuItems}}
   <div class="card" style="width: 18rem;">
            <img src="..." class="card-img-top" alt="Image photo">
            <div class="card-body">
                <p>Title</p>
                <h5 class="card-title">{{name}}</h5>
                <p class="card-text">{{description}}
                     
                </p>
                <a class="btn btn-primary" href="/">Add To Cart</a>
            </div>
        </div>
   {{/each}}
</div>